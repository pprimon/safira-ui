{
  "name": "safira-ui",
  "version": "0.1.0",
  "description": "Uma biblioteca de componentes React moderna, acessível e altamente customizável",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js"
    },
    "./theme": {
      "types": "./dist/theme/index.d.ts",
      "import": "./dist/theme/index.esm.js",
      "require": "./dist/theme/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "sideEffects": false,
  "keywords": [
    "react",
    "components",
    "ui",
    "design-system",
    "accessibility",
    "a11y",
    "typescript",
    "material-ui"
  ],
  "author": "Priscila Primon",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/pprimon/safira-ui.git"
  },
  "bugs": {
    "url": "https://github.com/pprimon/safira-ui/issues"
  },
  "homepage": "https://github.com/pprimon/safira-ui#readme",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "build:lib": "vite build --mode lib",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:a11y": "jest --testPathPattern=a11y",
    "prepublishOnly": "npm run build:lib"
  },
  "peerDependencies": {
    "react": ">=17.0.0",
    "react-dom": ">=17.0.0"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.4",
    "@mui/material": "^7.3.4"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^3.2.3",
    "@eslint/js": "^9.36.0",
    "@storybook/addon-a11y": "^8.6.15",
    "@storybook/addon-essentials": "^8.6.15",
    "@storybook/react-vite": "^8.6.15",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.9.0",
    "@types/react": "^18.3.26",
    "@types/react-dom": "^18.3.7",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/browser": "^3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9.36.0",
    "eslint-plugin-jsx-a11y": "^6.10.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.22",
    "eslint-plugin-storybook": "^0.11.1",
    "eslint-plugin-testing-library": "^7.0.0",
    "globals": "^16.4.0",
    "jest": "^30.2.0",
    "jest-axe": "^9.0.0",
    "jest-environment-jsdom": "^30.2.0",
    "jsdom": "^27.0.1",
    "playwright": "^1.56.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook": "^8.6.15",
    "ts-jest": "^29.4.6",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.45.0",
    "vite": "^6.4.1",
    "vitest": "^3.2.4"
  }
}
